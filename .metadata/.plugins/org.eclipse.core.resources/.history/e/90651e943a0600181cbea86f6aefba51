<suite name="Regression TestSuite">
<test name="GroupsDemo">
	<groups>
	<define name = "all">
		<include name = "bikes"></include>
		<include name = "sedan"></include>
		<include name = "suv"></include>
	</define>
	<define name = "nobikes">
		<include name = "suv"></include>
		<include name = "sedan"></include>
	</define>
		<run>
			<include name="all"></include>
			<include name = "suv"></include>
		</run>
	</groups>
	<classes>
		<class name="testclasses.TestNG_Grouping" />
	</classes>
</test>
</suite>

